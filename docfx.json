{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/toc.yml"
        ],
        "src": "content"
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "ANcpLua",
      "_appFooter": "ANcpLua Documentation",
      "_baseUrl": "/ANcpLua.io/",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_gitContribute": {
        "repo": "https://github.com/ANcpLua/ANcpLua.io",
        "branch": "main",
        "path": "content"
      }
    },
    "template": [
      "default",
      "modern"
    ]
  }
}