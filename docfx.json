{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "files": ["**/bin/Release/netstandard2.0/*.dll"],
          "src": ".repos/utilities"
        },
        {
          "files": ["**/bin/Release/netstandard2.0/*.dll"],
          "src": ".repos/analyzers"
        }
      ],
      "dest": "content/api",
      "includePrivateMembers": false,
      "disableDefaultFilter": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.md", "**/toc.yml"],
        "src": "content"
      },
      {
        "files": ["**/*.yml"],
        "src": "content/api"
      }
    ],
    "resource": [
      {
        "files": ["images/**", "**/*.png", "**/*.jpg", "**/*.svg"],
        "src": "content"
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "ANcpLua Framework",
      "_appName": "ANcpLua",
      "_appFooter": "ANcpLua Framework Documentation",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/ANcpLua/ANcpLua.io",
        "branch": "main"
      }
    },
    "template": ["default", "modern"],
    "sitemap": {
      "baseUrl": "https://ancplua.github.io",
      "changefreq": "weekly"
    }
  }
}
